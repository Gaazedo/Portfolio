  #F°/C°
def exibeFahrenheitCelsius(a):
  C0 = (a- 32) * 5/9


  return C0

def CelsiusFahrenheit(b):
  C2 = (b * 9/5) + 32

  return C2



Msg()

result = Convert()

T1 = float(input("Qual sua temperatura inicial? "))
T2 = float(input("Qual sua temperatura final? "))

if result == "F":
  exibeFahrenheitCelsius(T1,T2)
else:
  exibeCelsiusFahrenheit(T1,T2)
